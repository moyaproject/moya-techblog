<?xml version="1.0" encoding="UTF-8"?>
<moya xmlns="http://moyaproject.com"
    xmlns:let="http://moyaproject.com/let"
    xmlns:tn="http://moyaproject.com/thumbnail"
    xmlns:image="http://moyaproject.com/image">

    <tn:thumbnails libname="thumbnails.tests" name="admin">

        <tn:processor name="small" version="4">
            <image:resize-to-fit width="120" height="120"/>
        </tn:processor>

        <tn:processor name="admin" version="3">
            <image:resize-to-fit width="120" height="120" />
        </tn:processor>

        <tn:processor name="square_tile" version="3">
            <image:square/>
            <image:resize-to-fit width="140" height="140"/>
        </tn:processor>

        <tn:processor name="manager_tile" quality="70" version="6">
            <image:square/>
            <image:resize-to-fit width="200" height="200"/>
        </tn:processor>

        <tn:processor name="manager_form" version="2">
            <image:resize-to-fit width="320" height="240"/>
        </tn:processor>

        <tn:processor name="option" version="5" quality="70">
            <image:resize-to-fit width="128" height="96"/>
            <image:resize-canvas width="128" height="96" color="black"/>
        </tn:processor>

    </tn:thumbnails>

</moya>
