{
    "name": "Moya Tech Blog",
    "description": "A blogging application for coders and photographers",
    "repository": "https://github.com/moyaproject/moya-techblog",
    "success_url": "/",
    "keywords": [
        "moya",
        "blog",
        "blogging",
        "photography"
    ],
    "scripts": {
        "postdeploy": "moya init"
    },
    "env": {
        "MOYA_PROJECT": "./heroku",
        "MOYA_PROJECT_INI": "heroku.ini",
	"TECHBLOG_UPLOADS_LOCATION": "./__uploads__",
        "TECHBLOG_THUMBS_URL": "/uploads/"
    },
    "addons": [
        "heroku-postgresql:hobby-dev"
    ]
}
